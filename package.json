{
  "name": "doodle-this",
  "version": "1.0.0",
  "homepage": "https://JGEsteves89.github.io/doodle-this",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "type-check": "tsc",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "publish:patch": "npm run build && npm version patch && git push && git push --tags && npm run deploy",
    "publish:minor": "npm run build && npm version minor && git push && git push --tags && npm run deploy",
    "publish:major": "npm run build && npm version major && git push && git push --tags && npm run deploy"
  },
  "dependencies": {
    "caniuse-lite": "^1.0.30001731",
    "konva": "^10.0.8",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-icons": "^5.5.0",
    "react-konva": "^19.2.0",
    "react-zoom-pan-pinch": "^3.7.0"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "^5.5.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitejs/plugin-react": "^4.7.0",
    "eslint": "^9.38.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "gh-pages": "^6.3.0",
    "postcss": "^8.5.6",
    "pre-commit": "^1.0.10",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.46.2",
    "vite": "^7.1.12"
  },
  "pre-commit": "lint",
  "license": "MIT"
}
